;wildcard application [5] 
*.apps	IN	A	192.168.86.250 

;bootstrap node to start cluster install only [6] 
bootstrap  IN	A	192.168.86.200  

;master nodes [7] 
master1	IN	A	192.168.86.201  
;master2	IN	A	192.168.86.202  
;master3	IN	A	192.168.86.203 

;worker nodes [8] 
;worker1	IN	A	192.168.86.204 
;worker2 	IN	A	192.168.86.205